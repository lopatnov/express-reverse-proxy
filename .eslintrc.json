{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-use-before-define": "off",
        "linebreak-style": "off",
        "no-console": "off",
        "no-unused-vars": "warn"
    }
}