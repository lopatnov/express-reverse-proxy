{
  "port": 8080,
  "folders": "demo/client-a",
  "proxy": {
    "/api": "http://localhost:4001"
  },
  "headers": {
    "X-Powered-By": "express-reverse-proxy"
  },
  "unhandled": {
    "html": {
      "status": 307,
      "headers": {
        "Location": "/"
      }
    },
    "json": {
      "status": 404,
      "send": {
        "error": "Not Found"
      }
    }
  }
}
