{
  "name": "@lopatnov/express-reverse-proxy",
  "email": "oleksandr@lopatnov.cv.ua",
  "version": "5.0.0",
  "description": "Node.js CLI tool to serve static front-end files with a reverse proxy for back-end APIs",
  "type": "module",
  "author": "lopatnov",
  "main": "server.js",
  "exports": {
    ".": "./server.js",
    "./hot-reload-client": "./hot-reload-client.js"
  },
  "bin": {
    "express-reverse-proxy": "./server.js",
    "lerp": "./server.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "reverse",
    "proxy",
    "javascript",
    "express",
    "node",
    "cli",
    "static-server",
    "spa",
    "development"
  ],
  "scripts": {
    "start": "node ./server.js --config demo/server-config.json",
    "start:server-a": "node ./demo/server-a.js",
    "start:server-b": "node ./demo/server-b.js",
    "demo": "node ./scripts/demo.js",
    "test": "node ./scripts/test.js",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "lint": "biome check .",
    "format": "biome format --write .",
    "pm2-start": "pm2 start ecosystem.config.cjs --no-daemon",
    "pm2-restart": "pm2 restart ecosystem.config.cjs",
    "pm2-stop": "pm2 stop ecosystem.config.cjs",
    "pm2-status": "pm2 status",
    "pm2-logs": "pm2 logs --lines 200",
    "pm2-monitor": "pm2 monit",
    "demo:pm2-start": "pm2 start demo/ecosystem.config.cjs --no-daemon",
    "demo:pm2-restart": "pm2 restart demo/ecosystem.config.cjs",
    "demo:pm2-stop": "pm2 stop demo/ecosystem.config.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lopatnov/express-reverse-proxy.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lopatnov/express-reverse-proxy/issues"
  },
  "homepage": "https://lopatnov.github.io/express-reverse-proxy/",
  "files": [
    "server.js",
    "hot-reload-client.js",
    "ecosystem.config.cjs",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "express-basic-auth": "^1.2.1",
    "express-http-proxy": "^2.1.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pm2": "^6.0.14",
    "response-time": "^2.3.2",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "cypress": "^15.10.0"
  }
}
