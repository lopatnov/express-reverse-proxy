{
  "name": "@lopatnov/static-server-express",
  "version": "1.0.0",
  "description": "A back-end tool to serve front-end projects. Configure a tool and serve your projects.",
  "author": "lopatnov",
  "main": "server.js",
  "keywords": [
    "static",
    "server",
    "express"
  ],
  "scripts": {
    "start": "node ./server.js",
    "pm2-start": "pm2 start server.js --wait-ready --time --listen-timeout 10000",
    "pm2-stop": "pm2 stop server.js",
    "pm2-status": "pm2 status",
    "pm2-logs": "pm2 logs --lines 200"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lopatnov/static-server-express.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lopatnov/static-server-express/issues"
  },
  "homepage": "https://github.com/lopatnov/static-server-express#readme",
  "dependencies": {
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "morgan": "^1.10.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "pm2": "^5.1.2"
  }
}
